{
    "name": "JKQR",
    "version": "0.0.0.1",
    "manifest_version": 2,
    "description": "A web-based QR code extension that enables you create QR codes for free for any text or URL",
    "browser_action":{
        "default_popup": "popup.html",
        "default_icon": "logo.png"
    },
    "icons":{
        "128": "logo.png"
    },
    "permissions": ["activeTab"]
}